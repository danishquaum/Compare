//to add two numbers
#include<stdio.h>
getinput(int a,int b)
{
  printf("Enter the first number\n");
  scanf("%d",&a);
  printf("Enter the second number\n");
  scanf("%d",&b);
}
int add(int m,int n)
{
  int c=m+n;
  return c;
}
output(int p,int q)
{
    int s=add(p,q);
    printf("Sum=%d",s);
}
void main()
{
    int c,d;
    getinput(c,d);
    output(c,d);
}
